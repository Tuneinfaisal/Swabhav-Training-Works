	JButton botton = new JButton("Hello");
	
	MyFrame mf = new MyFrame();
	
	public static void main(String[] args)
	{
		
	}
	
	public MyFrame()
	{
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
