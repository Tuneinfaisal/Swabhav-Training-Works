package com.monocept;

import java.util.ArrayList;

public class Stock {
	
	private String name;
	private int num;
	ArrayList<IView> view = new ArrayList<IView>();
	
	public Stock(String name, int num)
	{
		this.name = name;
		this.num = num;
	}
	
	public void addStock(IView v)
	{
		view.add(v);
	}

}
