package com.decorator;

public class RibbonHat extends HatDecorator{

	public RibbonHat(Hat hat)
	{
		super(hat);
	}
	
	@Override
	public String getName()
	{
		return "Premium hat";
	}

	@Override
	public int getPrice()
	{
		return 200;
	}

	@Override
	public String getDescription()
	{
		return "Best premium hat";
	} 

}
